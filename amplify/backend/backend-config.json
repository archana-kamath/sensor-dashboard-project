{
  "function": {
    "projectsLambdaFnc": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "project",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    },
    "userLambdaFnc": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "user",
          "attributes": [
            "Name",
            "Arn"
          ],
          "attributeEnvMap": {
            "Name": "TABLE_NAME",
            "Arn": "TABLE_ARN"
          }
        }
      ]
    }
  },
  "storage": {
    "project": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "user": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  }
}